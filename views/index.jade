extends layout

block content
  .container
    h1= title
    p Home #{title}
    table
      - for(var i = 0 ; i < show_data_home.length ; i++)
          thead
           tr
            th(data-field="id") Name: #{show_data_home[i].user.name} - @#{show_data_home[i].user.screen_name}
           tbody
             tr
               td #{show_data_home[i].text}
